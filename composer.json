{
    "name": "be-mohand/cakephp-algolia",
    "description": "Algolia plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2",
        "algolia/algoliasearch-client-php": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Algolia\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Algolia\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
